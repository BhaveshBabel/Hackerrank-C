#include <stdio.h>
#include <string.h>

int main() {

    char s[1000];
    scanf("%s",&s);
    int i, j, count = 0;
    for(i = 48; i <= 57; i++){
        count = 0;
        for(j = 0; j < strlen(s); j++){
            if(s[j] == i){
                count++;
            }
        }
        printf("%d ", count);
    }
    
    return 0;
}
